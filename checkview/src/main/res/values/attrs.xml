<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckView">
        <attr name="radius" format="dimension"/>
        <attr name="checked" format="boolean"/>
        <attr name="checkedColor" format="color"/>
        <attr name="unCheckedColor" format="color"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="duration" format="integer"/>
        <attr name="shapeStyle" format="enum">
            <enum name="tick" value="1"/>
            <enum name="heart" value="2"/>
            <enum name="star" value="3"/>
        </attr>
    </declare-styleable>
</resources>